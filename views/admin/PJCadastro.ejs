<script src="/js/js/PJCadastro.js" defer></script>
<div class="container">
    <h1>Cadastrar Pessoa Jurídica</h1>
    <div id="form">
        <div class="form-floating mb-3">
            <input type="text" class="form-control" id="nomeval" placeholder=" " maxlength="100">
            <label for="nome">Nome:</label>
            <p id="erro-nome" class="text-danger"></p>
        </div>
        <div class="form-floating mb-3">
            <input type="text" class="form-control" id="telefoneval" placeholder=" " maxlength="11">
            <label for="telefone">Telefone:</label>
            <p id="erro-telefone" class="text-danger"></p>
        </div>
        <div class="form-floating mb-3">
            <input type="text" class="form-control" id="cnpjval" placeholder=" " maxlength="14">
            <label for="cnpj">CNPJ:</label>
            <p id="erro-cnpj" class="text-danger"></p>
        </div>
        <div class="form-floating mb-3">
            <input type="text" class="form-control" id="enderecoval" placeholder=" " maxlength="200">
            <label for="endereco">Endereço:</label>
            <p id="erro-endereco" class="text-danger"></p>
        </div>
        <div class="form-floating mb-3">
            <input type="text" class="form-control" id="emailval" placeholder=" " maxlength="100">
            <label for="email">Email:</label>
            <p id="erro-email" class="text-danger"></p>
        </div>
        <div class="form-floating mb-3">
            <input type="password" class="form-control" id="senhaval" placeholder=" " maxlength="30">
            <label for="senha">Senha:</label>
            <p id="erro-senha" class="text-danger"></p>
        </div>
        <div class="form-floating mb-3">
            <input type="password" class="form-control" id="senha2val" placeholder=" " maxlength="30">
            <label for="senha2">Confirme a Senha:</label>
            <p id="erro-senha2" class="text-danger"></p>
        </div>
        <p id="msg-final"></p>
        <button class="btn btn-primary" id="cadastrar">Cadastrar Cliente</button>
        <a id="voltar" href="/admin/Clientes" class="btn btn-secondary">Voltar</a>
    </div>
</div>